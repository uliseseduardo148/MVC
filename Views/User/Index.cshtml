@model IEnumerable<MVC.Models.tbl_user>

@{
    ViewBag.Title = "Usuarios";
}

<h2>Usuarios del sistema</h2>

<table class="table">
    <tr>
        <th>
           Nombre de usuario
        </th>
        <th>
           Contraseña
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fld_username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fld_encryptedPassword)
        </td>
    </tr>
}

</table>
